
<%- include ('partials/header.ejs') %>

<div class="container" style='margin:30px'>

</div>






<div class="container ">


<%thing.forEach(function(blog){%>
 <div class="card mb-3" style='width:70%'>
  
<img src='<%=blog.url%>' class="card-img-top" alt="...">
  <div class="card-body">
    <h5 class="card-title"><%=blog.title%></h5>
    <p class="card-text"><%=blog.post.substring(0,100)%></p>
    <p class="card-text"><small class="text-muted">Last updated <%= new Date(document.lastModified) %></small></p>
    <a href=<%='/blogs/'+blog._id%> class="btn btn-primary">Read More</a>
  </div>
</div>

<%})%>




</div>
<%- include ('partials/footer.ejs') %>